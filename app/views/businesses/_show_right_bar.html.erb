<div class="content position-relative tab-section bg-light">
  <ul class="nav nav-tabs" id="myTab" role="tablist">
    <li class="nav-item mt-3">
      <a class="nav-link active fs-20 mx-sm-4 header_tabs" id="notes-tab" data-toggle="tab" href="#notes-tab-body" role="tab" aria-controls="notes-tab-body" aria-selected="true">Notes</a>
    </li>
    <li class="nav-item mt-3 mr-auto">
      <a class="nav-link fs-20 mx-sm-4 header_tabs" id="venues-tab" data-toggle="tab" href="#venues-tab-body" role="tab" aria-controls="venues-tab-body" aria-selected="false">Venues</a>
    </li>
    <li class="mt-3 mt-md-0">
      <div class="ml-4 d-inline">
        <%= link_to "Create Note", "javascript:(0)", remote: true, class: 'btn btn-primary add_note' %>
      </div>
    </li>
      <li class="mt-3 mt-md-0 d-none">
      <div class="ml-4 d-inline">
        <%= link_to "Add Venue", new_business_venue_path(@business.id), remote: true, class: 'btn btn-primary add_venue' %>
      </div>
    </li>
  </ul>
  <div class="tab-content" id="myTabContent">
    <div class="tab-pane fade show active" id="notes-tab-body" role="tabpanel" aria-labelledby="notes-tab">
      <%= render 'notes_list' %>
    </div>
    <div class="tab-pane fade" id="venues-tab-body" role="tabpanel" aria-labelledby="venues-tab">
      <%= render 'venues_list', venues: @venues %>
    </div>
  </div>
</div>
