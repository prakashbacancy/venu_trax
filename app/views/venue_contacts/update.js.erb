$('.venue-contacts-data').html('<%= j render(partial: 'venue_contacts/list', locals: { venue_contacts: @venue_contacts, venue: @venue }) %>')
$('.sidebar_venue_contacts').html('<%= j render(partial: 'venue_contacts/sidebar_venue_contacts', locals: { venue_contacts: @venue_contacts, venue: @venue }) %>')

<% if params[:add_more].present? %>
  $('#right-modal-content').html('<%= j render(partial: 'form', locals: { venue_contact: @venue_contact_more, venue: @venue, add_more: true }) %>')
  $('#flashMessage').html("<%= j render 'layouts/flash_messages' %>");
  $("#right_modal").modal('show');
  $('.modal-content').scrollTop(0);
  $('form').parsley();
<% else %>
  $('.header_flash_message').html("<%= j render 'layouts/flash_messages' %>");
  $("#right_modal").modal('hide');
<% end %>