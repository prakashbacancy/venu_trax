<div class=" px-3 pt-3">
  <div class="">
    <div class="d-block d-flex align-items-center justify-content-between venue-contact venue-contact-header" >
      <div class="text-dark fw-500" data-toggle="collapse"
      href="#collapseContact" role="button" aria-expanded="true" aria-controls="collapseExample"><span class="mr-auto fs-16 fw-700 text-dark2 ">Venue Contacts(<%= venue_contacts.count %>)</span></span>
        <i class="fa fa-chevron-down fs-14 ml-2"></i></div>
      <% unless venue_contact?(current_user) %>
        <%= link_to "+ Add", new_venue_venue_contact_path(venue), class: "text-blue1 fw-600 fs-14 add_btn", remote: true %>
      <% end %>
    </div>
  </div>
</div>
<div class="collapse show" id="collapseContact">
  <div class="card card-body border-0">
    <%= render(partial: 'venue_contacts/sidebar_venue_contact', collection: venue_contacts) || no_data_available %>
  </div>
</div>
