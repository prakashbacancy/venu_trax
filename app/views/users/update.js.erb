<% if params[:add_more].present? %>
  $('#right-modal-content').html('<%= j render(partial: 'form', locals: {data: @data}) %>')
  $('#flashMessage').html("<%= j render 'layouts/flash_messages' %>");
  $("#right_modal").modal('show');
  $('.modal-content').scrollTop(0);
  $('form').parsley();
  $('.user_list_div').html('<%= j render(partial: 'user_list', locals: {users: @users}) %>')
<% else %>
  $(window.location.replace("<%= users_path %>"))
<% end %>
