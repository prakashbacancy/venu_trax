<% if meetings.present? %>
  <div class=" bg-white  mt-3 overflow-hidden">
  <% meetings.each do |meeting| %>
    <div class="shadow p-3 mb-3 bg-white rounded mt-4 note_shadow_box_5">
      <div class="d-flex align-items-start">
        <%= image_pack_tag 'meeting/ic_calendar.svg', class: 'mr-3 mt-1' %>
        <div class="meeting_info w-100">
          <div class="d-flex align-items-start justify-content-between flex-wrap">
            <div>
              <p class="text-dark2 fw-600 fs-16">Meet By: <%= meeting.title %>
              </p>
            </div>
            <p class="text-gray fs-14"><%= meeting.start_date_time.strftime('%b %d, %Y %I:%M %p') %></p>
          </div>
          <p>
          </p>
          <p class="text-gray mt-3 fs-16"><%= meeting.description.html_safe %>
          </p>
          <p class="text-dark2 fw-600 fs-16 mt-4"><%= meeting.title %>
          </p>
          <div class="d-flex align-items-center flex-wrap mt-4">
            <span class="mr-5">
              <p class="text-blue1 fw-600 fs-15">Outcome
              </p>
              <p class="text-darkgray1  fw-600 fs-16 mt-1">Scheduled
              </p>
            </span>
            <span class="mr-5" data-html="true" title="<%= meeting_attendees(meeting) %>">
              <p class="text-blue1  fw-600 fs-15">Attendees
              </p>
              <p class="text-darkgray1  fw-600  fs-16 mt-1"><%= meeting.attendees.count %> Attendees
              </p>
            </span>
            <span class="mr-5">
              <p class="text-blue1  fw-600 fs-15">Duration
              </p>
              <p class="text-darkgray1  fw-600  fs-16 mt-1">1 Hour
              </p>
            </span>
            <span class="mr-5">
              <p class="text-blue1  fw-600 fs-15">Date
              </p>
              <p class="text-darkgray1  fw-600  fs-15 mt-1"><%= meeting.date.strftime('%b %d, %Y') %>
              </p>
            </span>
          </div>
          <p class="text-dark2 fw-600 fs-16 mt-5">Team Notes
          </p>
          <p class="text-gray mt-2 fs-16"> <%= meeting.team_note.html_safe %>
          </p>
        </div>
      </div>
      <div class="mt-3">
        <%= link_to new_venue_meeting_comment_path(venue.id, meeting.id), remote: true, class: 'text-primary fs-16 fw-600 add_comment' do %>
          <%= image_pack_tag 'home/ic_comment.svg', class: 'img-fluid' %> Add Comment
        <% end %>
        | <a class="text-primary fs-16 fw-600 ml-2 text-nowrap" data-toggle="collapse" href="#meeting_comment_partial_<%= meeting.id %>" role="button" aria-expanded="false" aria-controls="meeting_comment_partial_<%= meeting.id %>">
          Comments <i class="fa fa-chevron-down"></i>
        </a>
      </div>
      <div class="add_comment_for_meeting_<%= meeting.id %> main_comment_form_div"></div>
      <div class="collapse show" id="meeting_comment_partial_<%= meeting.id %>">
        <% if meeting.comments.present? %>
          <% meeting.comments.each do |comment| %>
            <div class="card card-body mt-2">
              <p class="fs-18"><%= comment.description.html_safe %></p>
              <p class="text-gray mt-3 fs-16"> -- <%= "#{comment.user.full_name} | #{comment.created_at.strftime('%b %d, %Y %l:%M %p')}" %></p>
            </div>
          <% end %>
        <% else %>
          <div class="card card-body mt-2">
            <p class="text-center text-gray">No Comments yet</p>
          </div>
        <% end %>
      </div>
    </div>
  <% end %>
  </div>
<% else %>
  <div class="shadow p-3 mb-5 bg-white rounded mt-4">
    <p class="fs-18 text-center text-gray">No Data Available</p>
  </div>
<% end %>
