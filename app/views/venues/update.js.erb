<% if params[:add_more].present? %>
  $('#right-modal-content').html('<%= j render(partial: 'form', locals: {data: @data, group: @info_group}) %>')
  $('#flashMessage').html("<%= j render 'layouts/flash_messages' %>");
  $("#right_modal").modal('show');
  $('.modal-content').scrollTop(0);
  $('form').parsley();
<% else %>
  $('.header_flash_message').html("<%= j render 'layouts/flash_messages' %>");
  $("#right_modal").modal('hide');
<% end %>
<% if params[:venue][:group_id] %>
  $('#group-<%= params[:venue][:group_id] %>-tab-body').html('<%= j render(partial: 'group_fields', locals: {klass: Klass.venue, venue: @venue, group: venue_group(params[:venue][:group_id]) }) %>')
<% else %>
  <% if @business %>
    $('.venue_list_div_business').html('<%= j render(partial: 'businesses/venue_table', locals: {venues: @venues}) %>')
  <% else %>
    $('.venue_list_div').html('<%= j render(partial: 'venue_list', locals: {venues: @venues}) %>')
  <% end %>
<% end %>
