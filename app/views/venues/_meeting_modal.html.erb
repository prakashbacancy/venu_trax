<div class="modal fade meetingAttendeesModal" id="meetingAttendeesModal_<%= meeting.id %>" tabindex="-1" role="dialog" aria-labelledby="meetingAttendeesModalLabel" aria-hidden="true">
  <div class="modal-dialog modal-dialog-centered modal-lg" role="document">
    <div class="modal-content">
      <div class="modal-header bg-white1">
        <h5 class="modal-title text-dark" id="exampleModalLabel">Attendees List</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body px-5">
        <div class="row">
          <div class="col-md-12">
            <div class="admin-table bg-white table-responsive mt-3">
              <table class="table table-bordered w-100 mb-5 modal-table">
                <thead class="thead-dark">
                  <th width="40%">Full Name</th>
                  <th>Email Address</th>
                </thead>
                <% attendees = meeting_attendees_for_modal(meeting) %>
                <tbody>
                <% attendees.each do |attendee| %>
                  <tr>
                    <td><%= attendee.full_name %></td>
                    <td><%= attendee.email %></td>
                  </tr>
                <% end %>
                </tbody>
              </table>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
