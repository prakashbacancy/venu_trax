          <li class="m-0 mr-3">
            <a class="btn sharp fs-12 text-dark2" href="javascript:(0)">
              <i><%= image_pack_tag 'home/ic_events.svg' %></i>Events
            </a>
          </li>
        </ul>
      </div>
    </div>
  </div>
</div>
