<% attachments = @venue&.attachments || @attachable.attachments %>
	<div class="card card-body border-0">
	  <div class="row mb-2">
				<% if attachments.present? %>
					<% attachments.each do |attachment| %>
		    		<div class="col-md-12 text-gray fs-16 d-flex align-items-center">
				      <i class="fas fa-paperclip"></i>
				      <div class="ml-2">
				        <p class="fs-14 "><%= attachment.venue_file.filename.to_s %></p>
				      </div>
		    		</div>
			    <% end %>
				<% else %>
					<span> No Attachment found. </span>
				<% end %>
	  </div>
	</div>
