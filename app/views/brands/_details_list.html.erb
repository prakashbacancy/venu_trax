<div class="shadow p-0 mb-3 bg-white rounded detail_box">
  <div class="box-header border-bottom py-3">
    <div class="d-flex align-items-center justify-content-between flex-wrap">
      <div>
        <p class="text-dark2 fw-700 fs-16 text-uppercase">Brand Details</p>
      </div>
      <%#= link_to 'Edit', edit_business_path(business, group_id: group.id), class: 'btn btn-primary btn-sm', remote: true %>
    </div>
  </div>
	<div class="details-view">
	  <div class="py-3 position-relative tab-section ">
	    <div class="box-body">
	      <%# group.fields.each do |field|%>
	        <div class="row ">
	          <div class="col-md-4">
	            <span class="fw-600 fs-15 <%#= 'asterisk' if field.required? %>">Domain Name</span>
	          </div>
	          <div class="col-md-6">
	            <span class="text-gray fs-14">
	              <%= @brand.domain_name %>
	            </span>
	          </div>
	        </div>
	        <div class="row ">
	          <div class="col-md-4">
	            <span class="fw-600 fs-15 <%#= 'asterisk' if field.required? %>">Website URL</span>
	          </div>
	          <div class="col-md-6">
	            <span class="text-gray fs-14">
	              <%= @brand.website_url %>
	            </span>
	          </div>
	        </div>
	        <div class="row ">
	          <div class="col-md-4">
	            <span class="fw-600 fs-15 <%#= 'asterisk' if field.required? %>">City</span>
	          </div>
	          <div class="col-md-6">
	            <span class="text-gray fs-14">
	              <%= @brand.city %>
	            </span>
	          </div>
	        </div>

	        <div class="row ">
	          <div class="col-md-4">
	            <span class="fw-600 fs-15 <%#= 'asterisk' if field.required? %>">State</span>
	          </div>
	          <div class="col-md-6">
	            <span class="text-gray fs-14">
	              Test
	            </span>
	          </div>
	        </div>
	        <div class="row ">
	          <div class="col-md-4">
	            <span class="fw-600 fs-15 <%#= 'asterisk' if field.required? %>">Phone Number</span>
	          </div>
	          <div class="col-md-6">
	            <span class="text-gray fs-14">
	              <%= @brand.phone_number %>
	            </span>
	          </div>
	        </div>
	        <div class="row ">
	          <div class="col-md-4">
	            <span class="fw-600 fs-15 <%#= 'asterisk' if field.required? %>">No of Employee</span>
	          </div>
	          <div class="col-md-6">
	            <span class="text-gray fs-14">
	              <%= @brand.no_of_employee %>
	            </span>
	          </div>
	        </div>
	        <div class="row ">
	          <div class="col-md-4">
	            <span class="fw-600 fs-15 <%#= 'asterisk' if field.required? %>">Annual Revenue</span>
	          </div>
	          <div class="col-md-6">
	            <span class="text-gray fs-14">
	              <%= @brand.annual_revenue %>
	            </span>
	          </div>
	        </div>
	        <div class="row ">
	          <div class="col-md-4">
	            <span class="fw-600 fs-15 <%#= 'asterisk' if field.required? %>">Brand Owner</span>
	          </div>
	          <div class="col-md-6">
	            <span class="text-gray fs-14">
	              <%= @brand.brand_owner %>
	            </span>
	          </div>
	        </div>
	      <%# end %>
	    </div>
	  </div>
	</div>
</div>
