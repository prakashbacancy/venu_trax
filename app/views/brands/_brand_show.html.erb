<div class="d-flex align-items-center flex-wrap justify-content-between mt-3 search-card brand-top-bar">
  <div class="my-1">
    <%= link_to "Edit Brand", edit_brand_path(brand, inside: true), remote: true, class: 'btn btn-primary my-1' %>
    <%= link_to "Add Brand", new_brand_path(venue, inside: true), remote: true, class: 'btn btn-primary ml-2 my-1' %>
  </div>
</div>
<div class="py-3 position-relative tab-section shadow mb-3 bg-white rounded mt-2">
  <div class="d-flex align-items-center justify-content-between">
    <ul class="nav nav-tabs mb-0" id="myTab" role="tablist">
      <li class="nav-item">
        <a class="nav-link fs-18 mx-sm-2 header_tabs active sub_brand_tabs" id="brand-detail-tab" data-toggle="tab"
          href="#brand-detail-tab-body" role="tab" aria-controls="brand-detail-tab-body"
          aria-selected="false">Brand Details
        </a>
      </li>
      <li class="nav-item">
        <a class="nav-link fs-18 mx-sm-2 header_tabs sub_brand_tabs cursor-not-allowed" id="brand-contact-tab" data-toggle="tab"
          href="#brand-contact-tab-body" role="tab" aria-controls="brand-contact-tab-body"
          aria-selected="true">Brand Contacts
        </a>
      </li>
    </ul>
  </div>
  <div class="tab-content" id="myTabContent">
    <div class="tab-pane fade  active show" id="event-detail-tab-body" role="tabpanel"
      aria-labelledby="brand-detail-tab">
      <%= render 'brand_details_tab', brand: brand %>
    </div>
  </div>
</div>