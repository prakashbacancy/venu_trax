<div class="row">
  <div class="col-md-6 mb-4">
    <div class="box-body admin-box bg-white p-4 h-100 rounded-10 table-bordered border-radius-5">
      <div class="chart-header">
        <h5 class="chart-main-title mb-0">Estimated Wifi Landing Page Login Attendance</h5>
        <p class="chart-sub-title text-blue3 mb-3"></p>
        <div>
          <%= column_chart(@simulations[:wifi_lp_login], id: 'wifi_lp') %>
        </div>
        <div class="chart-labels row justify-content-around mt-4 border rounded px-0 py-3 text-gray fs-14 text-break">
          <span class="col-6 col-sm-6 col-xl-3 ">Daily<br> <b><%= number_with_delimiter(@simulations[:daily_wifi_lp_login].to_i, :delimiter => ',') %></b></span>
          <span class="col-6 col-sm-6 col-xl-3 ">Weekly<br><b><%= number_with_delimiter(@simulations[:week_wifi_lp_login].to_i, :delimiter => ',') %></b></span>
          <span class="col-6 col-sm-6 col-xl-3 ">Monthly<br> <b><%= number_with_delimiter(@simulations[:month_wifi_lp_login].to_i, :delimiter => ',') %></b></span>
          <span class="col-6 col-sm-6 col-xl-3 ">Annually<br><b><%= number_with_delimiter(@simulations[:year_wifi_lp_login].to_i, :delimiter => ',') %></b></span>
        </div>
      </div>
    </div>
  </div>
  <div class="col-md-6 mb-4">
    <div class="box-body admin-box bg-white p-4 h-100 rounded-10 table-bordered border-radius-5">
