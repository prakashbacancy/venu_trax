<%= form_tag field_picklist_values_path, method: :post do %>
  <%= label_tag "Select Field","Select Field", class:'select-module-set text-left text-capitalize fs-17 m-0 mb-1 fw-600'  %>
  <%= select_tag :field_id, options_for_select(Klass.find_by(id: klass.id).field_picklist_valuable_fields.pluck(:name, :id)), { class: ' form-control', include_blank: 'Select Field' } %>
  <%#= grouped_collection_select nil, :field_id, Klass.where(id: klass.id), :field_picklist_valuable_fields, :label, :id, :label, {include_blank: "Select Field"}, class: 'form-control' %>
<%end%>
<script>
  $(document).ready(function(){
    $("#field_id").on('change', function(){
      $('#picklist-values').html('');
      $.ajax({
        url: "<%= change_field_field_picklist_values_url %>",
        type: 'GET',
        data: {
          klass_id: $("#klasses").val(),
          field_id: $(this).val()
        },
        dataType: "script",
        success: function(result){

        }
      });
    });
  });
</script>
